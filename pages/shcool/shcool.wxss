/* 蒙版 */
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.7;
}

.modalDlg {
  width: 580rpx;
  height: 450rpx;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  margin: -370rpx 85rpx;
  background-color: #fff;
  border-radius: 36rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.close_mask {
  color: #fff;
  padding: 10rpx;
  margin-top: 20rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 100%;
  position: relative;
  left: 39%;
  font-size: 45rpx;
  background-color: #0278AE;
  text-align: center;
  line-height: 31rpx;
}


