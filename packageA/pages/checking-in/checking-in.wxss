.main {
  position: absolute;
  width: 100%;
  height: 100%;
  display: block;
  box-sizing: border-box;
  top: 0;
}

.list1{
  margin: 2%;
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  border: 1rpx solid #c8eef8;
  border-radius: 20rpx;
}

.list_techer{
  width: 72%;
}

.list_img{
  width: 70rpx;
  height: 70rpx;
  border-radius: 100%;
  margin-right: 20rpx;
}

.list_name{
  text-align: start;
  font-size: 28rpx;
  font-weight: 550;
}

.list_time{
  font-size: 25rpx;
  text-align: start;
}

