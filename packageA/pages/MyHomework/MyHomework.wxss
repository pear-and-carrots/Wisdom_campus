.page{
    margin: 30rpx;
    padding: 20rpx;
    background-color: #fff;
    border-radius: 20rpx;
    border: 1rpx solid #b7e2f3;
}

.course {
    font-size: 45rpx;
    font-weight: 700;
    color: #12779e;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 20rpx;
}

.content {
    margin: 20rpx;
    padding: 15rpx;
    background-color: #dcf5ff;
    border-radius: 15rpx;
}

.Hbox{
    margin: 20rpx;
    padding: 20rpx;
    background-color: #dcf5ff;
    border-radius: 15rpx;
}

.zuoye {
    padding-bottom: 10rpx;
}

.WyHomweorkBox{
    margin-top: 20rpx;
    margin-bottom: 20rpx;
}

.button {
    position: relative;
    top: 10%;
    background-color: rgb(71, 176, 236);
    color: #fff;
}

.notComplete{
    padding: 20rpx;
    background-color: #fff;
    border: 1rpx solid #b3e5f8;
    display: flex;
    flex-direction: row;
    align-items: center;
    text-align: center;
    font-weight: 550;
    color: #666666;
    margin: 2%;
    border-radius: 60rpx;
}

.notComplete_img{
    width: 70rpx;
    height: 70rpx;
    border-radius: 100%;
    margin-right: 20rpx;
}
.WyHomweorkImg{
    width: 100%;
}