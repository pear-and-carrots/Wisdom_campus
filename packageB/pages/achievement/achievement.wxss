Page {
  background-color: rgb(240, 251, 255);

}

.head_item {
  width: 374rpx;
  text-align: center;
  font-size: 34rpx;
  font-weight: 550;
  color: #999;
  letter-spacing: 0;
}

.head_itemActive {
  color: #197791;
  color: #fff;
}

.ring {
  width: 2rpx;
  height: 100%;
  /* background-color: rgba(204, 204, 204, 1); */
}

.head {
  width: 100%;
  height: 100rpx;
  background-color: rgba(255, 255, 255, 1);
  border-bottom: 1rpx solid rgba(204, 204, 204, 1);
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: fixed;
  top: 0;
  z-index: 10;
}

.main {
  position: absolute;
  width: 100%;
  height: 100%;
  display: block;
  box-sizing: border-box;
  padding-top: 100rpx;
  top: 0;
}

.show {
  display: block;
  text-align: center;
}

.hidden {
  display: none;
  text-align: center;
}

.search-box {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.bx{
  width: 70%;
  margin-left: 2%;
  margin-right: 2%;
}

.pricer {
  display: flex;
  width: 100%;
  padding: 12rpx;
  margin-top: 20rpx;
  margin-left: 2%;
  margin-right: 2%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 20rpx;
  border: 2rpx solid #8888;
  font-size: 28rpx;
  color: #888
}

.button {
  height: 65rpx;
  display: flex;
  width: 25%;
  margin-top: 15rpx;
  background-color: #0278AE;
  color: #fff;
}

.jieguo {
  height: 400px;
  width: 80%;
  background-color: rgb(143, 200, 202);
  border-radius: 20rpx;
  position: relative;
  left: 10%;
}

.list {
  margin: 2%;
  display: flex;
  flex-direction: row;
  border-radius: 20rpx;
  padding: 15rpx;
  background-color: #fff;
  border-radius: 20rpx;
  border: 1rpx solid #8888;
}

.list1 {
  width: 75%;
}

.list2 {
  width: 200rpx;
  font-weight: 550;
  font-size: 32rpx;
  justify-content: start;
  text-align: left;
}

.list3 {
  margin-left: 3%;
  width: 65rpx;
  padding: 8rpx;
  background-color: green;
  font-size: 25rpx;
  color: #fff;
  border-radius: 20rpx;
  text-align: center;
  margin-top: 20rpx;
}

.list4 {
  width: 25%;
}

.list5 {
  background-color: rgb(255, 254, 254);
  font-size: 28rpx;
  box-shadow: 1rpx 1rpx 1rpx 1rpx rgba(152, 165, 209, 0.533);
  margin-bottom: 5%;
  text-align: center;
  border-radius: 20rpx;

}

.list6 {
  color: red;
}

